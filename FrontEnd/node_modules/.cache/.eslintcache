l(interval);\r\n        // eslint-disable-next-line react-hooks/exhaustive-deps\r\n    }, [pageRefresh])\r\n\r\n\r\n    // pageRefresh, ForwardTo\r\n\r\n    // function displayConsults() {\r\n    //     return (\r\n    //         <select\r\n    //             // value={ForwardTo}\r\n    //             onChange={e => setForwardTo(e.target.value)}\r\n    //         >\r\n    //             <option value=\"\">Select Consultatnt</option>\r\n    //             {\r\n    //                 consultantList.map(\r\n    //                     item => <option key={item._id} value={item._id}>{`${item.first_name} ${item.last_name}`}</option>\r\n    //                 )\r\n    //             }\r\n    //         </select>\r\n    //     )\r\n    // }\r\n    function displayUserRequests() {\r\n        if (usersRequests.length === 0) {\r\n            return (\r\n                <h1>NO requests to show</h1>\r\n            )\r\n        } else {\r\n            return (\r\n                //-----------------------------------------\r\n                <table id=\"customers\">\r\n                    <tbody>\r\n                        <tr>\r\n                            <th>User</th>\r\n                            <th>Topic</th>\r\n                            <th>Urgency</th>\r\n                            <th>Forward To</th>\r\n                            <th>Date</th>\r\n                            <th>Action</th>\r\n                        </tr>\r\n                        {\r\n                            usersRequests.map(\r\n                                item =>\r\n                                    <tr key={item._id}>\r\n                                        <td>{item.user_name}</td>\r\n                                        <td>{item.topic}</td>\r\n                                        <td>{item.urgency}</td>\r\n                                        <td>\r\n                                            <select\r\n                                                className='DropLis